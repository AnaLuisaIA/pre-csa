		<property name="otroBono9">
			<column name="otroBono9" sql-type="NUMERIC" />
		</property>
		<property name="otroBono10">
			<column name="otroBono10" sql-type="NUMERIC" />
		</property>


	</class>

</hibernate-mapping>
